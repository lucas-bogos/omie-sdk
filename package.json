{
  "name": "omie-sdk",
  "version": "0.6.0",
  "description": "SDK da Omie para Node.js",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14.17.0"
  },
  "packageManager": ">=npm@6.0.0",
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf ./dist && tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "docs:generate": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucas-bogos/omie-sdk.git"
  },
  "keywords": [
    "omie-sdk",
    "integração-omie",
    "sdk-omie",
    "api-omie",
    "omie-plataforma",
    "contabilidade-online",
    "sistema-erp",
    "integração-api",
    "automação-financeira",
    "gestão-empresarial",
    "desenvolvimento-sdk",
    "plataforma-nuvem",
    "software-de-negócios",
    "integração-ERP",
    "contabilidade-digital",
    "automação-de-processos",
    "solução-financeira",
    "desenvolvimento-de-software",
    "omie-api-sdk",
    "ferramentas-de-negócios"
  ],
  "author": "Lucas Bogos <@lucas-bogos>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucas-bogos/omie-sdk/issues"
  },
  "homepage": "https://github.com/lucas-bogos/omie-sdk#readme",
  "files": [
    "./dist",
    "!/dist/.tsbuildinfo",
    "/LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^20.8.5",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "prettier": "3.0.3",
    "tsup": "^7.2.0",
    "typedoc": "^0.25.2",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2"
  }
}
